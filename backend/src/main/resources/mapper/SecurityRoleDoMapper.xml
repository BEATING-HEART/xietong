<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xietong.mapper.StaffDOMapper">
    <select id="findAll"  resultType="SecurityRoleDo">
        SELECT * FROM security_role
    </select>
    <select id="findOneById" resultType="SecurityRoleDo" >
        SELECT * FROM  staff where id=#{id}
    </select>
    <delete id="removeOne"  >
        delete from security_role where id=#{id}
    </delete>
    <insert id="insert" parameterType="SecurityRoleDo">
         insert into security_role(id,name,code,remark,status) values(#{})
    </insert>
    <resultMap id="StaffDOMap" type="StaffDo">
        <id column="staff_id" property="stuffId"/>
        <result column="staff_name" property="stuffName"/>
        <result column="staff_position" property="stuffPosition"/>
    </resultMap>
</mapper>